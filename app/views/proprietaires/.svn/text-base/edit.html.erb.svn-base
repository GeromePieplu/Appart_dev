<h1>Editing proprietaire</h1>

<% form_for(@proprietaire) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nom %><br />
    <%= f.text_field :nom %>
  </p>
  <p>
    <%= f.label :prenom %><br />
    <%= f.text_field :prenom %>
  </p>
  <p>
    <%= f.label :adresse %><br />
    <%= f.text_area :adresse %>
  </p>
  <p>
    <%= f.label :code_postal %><br />
    <%= f.text_field :code_postal %>
  </p>
  <p>
    <%= f.label :ville %><br />
    <%= f.text_field :ville %>
  </p>
  <p>
    <%= f.label :telephone %><br />
    <%= f.text_field :telephone %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :pays %><br />
    <%= f.text_field :pays %>
  </p>
  <p>
    nom du fichier de la signature <br />
    <%= f.text_field :signature %>
  </p>  
  <p>
    <%= f.label :login_name %> <br />
    <%= f.text_field :login_name %>
  </p>      
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @proprietaire %> |
<%= link_to 'Back', proprietaires_path %>